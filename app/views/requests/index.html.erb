<table>
  <tr>
    <th>Creation Date</th>
    <th>Status</th>
    <th>Type of Service Request</th>
    <th>Street Address</th>
  </tr>
  <% @requests.each do |request| %>
    <tr>
      <td><%= request['Creation Date'] %></td>
      <td><%= request['Status'] %></td>
      <td><%= request['Type of Service Request'] %></td>
      <td><%= request['Street Address'] %></td>
    </tr>
  <% end %>
</table>