<ol class="show-requests">
  <% requests.each do |request| %>
    <li>
      <%= link_to(request.street_address, request_path(request.id)) %>
      <span>reported on
      <%= request.creation_date.strftime("%m/%d/%Y") %></span>
    </li>
  <% end %>
</ol>
<%= will_paginate requests, class: 'digg_pagination' %>